{
  "swagger": {
    "outputDirectory": "./src",
    "basePath": "/api/v1",
    "entryFile": "./src/index.ts",
    "specVersion": 3,
    "noImplicitAdditionalProperties": "silently-remove-extras",
    "host": null,
    "schemes": ["http", "https"],
    "controllerPathGlobs": [
      "./**/*.controller.ts"
    ]
  },
  "routes": {
    "routesDir": "./src/routes",
    "basePath": "/api/v1",
    "entryFile": "./src/index.ts",
    "controllerPathGlobs": [
      "./**/*.controller.ts"
    ]
  }
}
